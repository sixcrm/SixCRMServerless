[
  {
    "DomainName": "sixcrm-logs",
    "ElasticsearchClusterConfig": {
      "InstanceCount": 1,
      "InstanceType": "m3.medium.elasticsearch"
    },
    "AccessPolicies":{
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Principal": {
            "AWS": "*"
          },
          "Action": "es:*",
          "Resource": "arn:aws:es:{{aws_account_region}}:{{aws_account_id}}:domain/{{domain_name}}/*"
        }
      ]
    }
  },
  {
    "DomainName": "sixcrm-dynamoaudit",
    "ElasticsearchClusterConfig": {
      "InstanceCount": 1,
      "InstanceType": "m3.medium.elasticsearch"
    },
    "AccessPolicies":{
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Principal": {
            "AWS": "*"
          },
          "Action": "es:*",
          "Resource": "arn:aws:es:{{aws_account_region}}:{{aws_account_id}}:domain/{{domain_name}}/*"
        }
      ]
    }
  }
]
