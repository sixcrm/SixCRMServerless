{
	"title": "Order",
	"type": "object",
	"properties": {
		"campaign_id":{
			"type":"string"
		},
		"session_id":{
			"type":"string"
		},
		"product_schedules":{
			"type": "array",
            "minItems": 1,
            "items": { "type": "string" },
            "uniqueItems": true
		},
		"name":{
			"type":"string"
		},
		"ccnumber":{
			"type":"string"
		},
		"ccexpiration":{
			"type":"string",
			"pattern": "^[0-9]{4}$"
		},
		"ccccv":{
			"type":"string"
		},
		"address":{
			"type":"object",
			"properties":{
				"line1":{
					"type":"string"
				},
				"line2":{
					"type":"string"
				},
				"city":{
					"type":"string"
				},
				"state":{
					"type":"string"
				},
				"zip":{
					"type":"string"
				},
				"country":{
					"type":"string"
				}
			},
			"required":["line1","city","state","zip","country"]
		}
	},
	"required":["ccnumber","ccexpiration", "ccccv", "product_schedules", "campaign_id", "session_id", "name"]
}
