{
  "$schema": "http://json-schema.org/draft-04/schema",
	"id":"/processenv.json",
	"title": "Process Env",
	"type": "object",
  "properties":{
    "origin_queue":{
      "type":"string"
    },
    "name":{
      "type":"string"
    },
    "workerfunction":{
      "type":"string"
    },
    "destination_queue":{
      "type":"string"
    },
    "failure_queue":{
      "type":"string"
    },
    "bulk":{
      "type":"boolean"
    }
  },
  "required":["name", "workerfunction"]
}
