{
	"$schema": "http://json-schema.org/draft-04/schema",
	"id":"/site",
	"title": "Site",
	"type": "object",
	"properties": {
		"email":{
			"type":"string",
			"format":"email"
		},
		"email_verified":{
			"type":"boolean"
		},
		"picture":{
			"anyOf":[
				{"type":"string", "format":"uri"},
				{"type":"string", "enum":[""]}
			]
		},
		"iss":{
			"type":"string",
			"format":"uri"
		},
		"sub":{
			"type":"string"
		},
		"aud":{
			"type":"string"
		},
		"exp":{
			"type":"integer"
		},
		"iat":{
			"type":"integer"
		}
	},
	"required":["email", "exp", "iat"]
}
