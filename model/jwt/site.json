{
	"$schema": "http://json-schema.org/schema#",
	"id":"/SixCRMTrackerObject",
	"title": "Tracker",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		},
		"OptionalUUIDv4":{
			"anyOf":[
				{"type":"string", "enum":[""]},
				{"$ref": "#/definitions/UUIDv4"}
			]
		}
	},
	"properties": {
		"email":{
			"type":"string",
			"format":"email"
		},
		"email_verified":{
			"type":"boolean"
		},
		"picture":{
			"anyOf":[
				{"type":"string", "format":"uri"},
				{"type":"string", "enum":[""]}
			]
		},
		"iss":{
			"type":"string",
			"format":"uri"
		},
		"sub":{
			"type":"string"
		},
		"aud":{
			"type":"string"
		},
		"exp":{
			"type":"integer"
		},
		"iat":{
			"type":"integer"
		}
	},
	"required":["email", "exp", "iat"]
}
