{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/functional/Innovio/process.json",
	"title": "Innovio Merchant Provider Process Object",
	"type": "object",
	"properties": {
		"req_username": {
			"type": "string"
		},
		"req_password": {
			"type": "string"
		},
		"request_action": {
			"type": "string",
			"enum": [
				"CCAUTHCAP"
			]
		},
		"site_id": {
			"type": "string"
		},
		"merchant_acct_id": {
			"type": "string"
		},
		"cust_fnam": {
			"type": "string"
		},
		"cust_lname": {
			"type": "string"
		},
		"cust_email": {
			"type": "string",
			"format": "email"
		},
		"cust_login": {
			"anyOf": [
				{
					"type": "string",
					"format": "ipv4"
				},
				{
					"type": "string",
					"format": "ipv6"
				}
			]
		},
		"cust_password": {
			"type": "string"
		},
		"xtl_cust_id": {
			"type": "string"
		},
		"xtl_order_id": {
			"type": "string"
		},
		"li_prod_id_1": {
			"type": "string"
		},
		"li_value_1": {
			"$ref": "../../definitions/currency.json"
		},
		"li_count_1": {
			"$ref": "../../definitions/nonnegativeinteger.json"
		},
		"bill_addr": {
			"type": "string"
		},
		"bill_addr_city": {
			"type": "string"
		},
		"bill_addr_state": {
			"type": "string"
		},
		"bill_addr_zip": {
			"type": "string"
		},
		"bill_addr_country": {
			"type": "string"
		},
		"pmt_numb": {
			"type": "string"
		},
		"pmt_key": {
			"type": "string"
		},
		"pmt_expiry": {
			"type": "string"
		},
		"request_response_format": {
			"type": "string"
		},
		"xtl_ip": {
			"type": "string"
		},
		"request_api_version": {
			"type": "string"
		}
	},
	"required": [
		"request_action",
		"request_response_format",
		"request_api_version",
		"req_username",
		"req_password",
		"site_id",
		"merchant_acct_id",
		"li_prod_id_1",
		"li_value_1",
		"li_count_1",
		"pmt_numb",
		"pmt_expiry"
	]
}
