{
	"$schema": "http://json-schema.org/schema#",
	"id":"/User",
	"title": "User",
	"type": "object",
	"definitions": {
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		},
		"SHA1": {
			"type": "string",
			"pattern":"^[0-9a-f]{5,40}$"
		}
	},
	"properties": {
		"id":{
			"type":"string",
			"format":"email"
		},
		"name":{
			"type":"string"
		},
		"first_name":{
			"type":"string"
		},
		"last_name": {
			"type":"string"
		},
		"auth0_id":{
			"type":"string"
		},
		"active":{
			"type":"string"
		},
		"termsandconditions":{
			"type":"string"
		},
		"alias":{"$ref": "#/definitions/SHA1"},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":["id", "name", "alias", "created_at", "updated_at"]
}
