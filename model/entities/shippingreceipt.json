{
	"$schema": "http://json-schema.org/schema#",
	"id":"/shippingreceipt",
	"title": "Shipping Receipt",
	"type": "object",
	"description": "A shipping receipt",
	"properties": {
		"id":{"$ref": "../definitions/uuidv4.json"},
		"account":{"$ref": "../definitions/sixcrmaccountidentifier.json"},
		"history":{
			"type":"array",
			"items":{"$ref":"./components/shippingreceipt/historyelement.json"}
		},
		"fulfillment_provider":{"$ref": "../definitions/uuidv4.json"},
		"fulfillment_provider_reference":{
			"type":"string"
		},
		"tracking":{
			"type":"object",
			"properties":{
				"id":{
					"type":"string"
				},
				"carrier":{
					"type":"string",
					"enum":["USPS","UPS","FedEx","Test","DHL"]
				}
			},
			"required":["id","carrier"]
		},
		"status":{
			"type":"string"
		},
		"created_at":{"$ref": "../definitions/iso8601.json"},
		"updated_at":{"$ref": "../definitions/iso8601.json"}
	},
	"required":["account", "fulfillment_provider", "status", "created_at", "updated_at"]
}
