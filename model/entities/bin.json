{
	"$schema": "http://json-schema.org/schema#",
	"id":"/bin",
	"title": "BIN",
	"type": "object",
	"description": "A credit card BIN",
	"properties": {
		"binnumber":{
			"type":"number",
			"minLength":6,
			"maxLength": 6
		},
		"brand":{
			"type":"string",
			"minLength":3
		},
		"bank":{
			"type":"string",
			"minLength":3
		},
		"type":{
			"type":"string",
			"minLength":3
		},
		"country":{
			"type":"string",
			"minLength":3
		},
		"info":{
			"type":"string",
			"minLength":3
		},
		"country_iso":{
			"type":"string",
			"minLength":2,
			"maxLength": 2
		},
		"country_iso2":{
			"type":"string",
			"minLength":3,
			"maxLength": 3
		},
		"country_iso3":{
			"type":"number",
			"minLength":3,
			"maxLength": 3
		},
		"webpage":{
			"type":"string",
			"minLength":3
		},
		"phone":{
			"type":"string",
			"minLength":3
		}
	},
	"required":["binnumber", "brannd", "bank", "country", "info", "country_iso","country_iso2" ]
}
