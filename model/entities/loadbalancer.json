{
	"$schema": "http://json-schema.org/schema#",
	"id":"/loadbalancer",
	"title": "Load Balancer",
	"type": "object",
	"definitions": {
		"UUIDv4": {"$ref": "model/definitions/uuidv4.json"},
		"ISO8601": {"$ref": "model/definitions/iso8601.json"},
		"SixAccountIdentifier":{"$ref": "model/definitions/sixcrmidentifier.json"}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"account":{"$ref": "#/definitions/SixAccountIdentifier"},
		"merchantproviders":{
			"type":"array",
			"items":{
				"type":"object",
				"properties":{
					"id":{"$ref": "#/definitions/UUIDv4"},
					"distribution":{
						"type":"number"
					},
					"required":["id", "distribution"]
				}
			}
		},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":["account", "created_at", "updated_at"]
}
