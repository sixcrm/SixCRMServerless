{
	"$schema": "http://json-schema.org/schema#",
	"id":"/rebill",
	"title": "Rebill",
	"type": "object",
	"properties": {
		"id":{"$ref": "../definitions/uuidv4.json"},
		"account":{"$ref": "../definitions/sixcrmidentifier.json"},
		"amount":{"$ref": "../definitions/currency.json"},
		"bill_at":{"$ref": "../definitions/iso8601.json"},
		"created_at":{"$ref": "../definitions/iso8601.json"},
		"updated_at":{"$ref": "../definitions/iso8601.json"},
		"parentsession":{"$ref": "../definitions/uuidv4.json"},
		"processing":{
			"type":["string", "boolean"]
		},
		"product_schedules":{
			"type":"array",
			"items":{"$ref": "../definitions/uuidv4.json"}
		},
		"transactions":{
			"type":"array",
			"items":{"$ref": "../definitions/uuidv4.json"}
		}

	},
	"required":["account", "amount", "bill_at", "created_at", "updated_at", "parentsession", "product_schedules"]
