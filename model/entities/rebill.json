{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/entities/rebill.json",
	"title": "Rebill",
	"type": "object",
	"description": "A rebill",
	"properties": {
		"id": {
			"$ref": "../definitions/uuidv4.json"
		},
		"account": {
			"$ref": "../definitions/sixcrmaccountidentifier.json"
		},
		"amount": {
			"$ref": "../definitions/currency.json"
		},
		"bill_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"created_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"updated_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"parentsession": {
			"$ref": "../definitions/uuidv4.json"
		},
		"merchant_provider": {
			"$ref": "../definitions/uuidv4.json"
		},
		"processing": {
			"type": [
				"string",
				"boolean"
			]
		},
		"product_schedules": {
			"type": "array",
			"items": {
				"$ref": "../definitions/uuidv4.json"
			}
		},
		"products": {
			"type": "array",
			"items": {
				"$ref": "./components/transactionproduct.json"
			}
		},
		"state": {
			"type": "string"
		},
		"previous_state": {
			"type": "string"
		},
		"state_changed_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"history": {
			"type": "array",
			"items": {
				"$ref": "./components/rebillstatehistoryelement.json"
			}
		},
		"second_attempt": {
			"type": "boolean"
		},
		"no_process": {
			"type": "boolean"
		},
		"upsell": {
			"$ref": "../definitions/uuidv4.json"
		},
		"paid": {
			"type": "object",
			"properties": {
				"detail": {
					"type": "string",
					"enum": [
						"full",
						"partial"
					]
				},
				"updated_at": {
					"$ref": "../definitions/iso8601.json"
				}
			}
		}
	},
	"required": [
		"account",
		"amount",
		"bill_at",
		"created_at",
		"updated_at",
		"parentsession"
	]
}
