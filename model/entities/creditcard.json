{
	"$schema": "http://json-schema.org/draft-04/schema",
	"id":"/entities/creditcard.json",
	"title": "Credit Card",
	"type": "object",
	"description": "A credit card object",
	"properties": {
		"id":{"$ref": "../definitions/uuidv4.json"},
		"account":{"$ref": "../definitions/sixcrmaccountidentifier.json"},
		"customers":{
			"type": "array",
			"items": {"$ref": "../definitions/uuidv4.json"}
		},
		"address":{"$ref": "../definitions/address.json"},
		"number":{"$ref": "../definitions/creditcardnumber.json"},
		"last_four":{"$ref":"../definitions/creditcardlastfour.json"},
		"ccv":{"$ref": "../definitions/creditcardccv.json"},
		"expiration":{"$ref": "../definitions/creditcardexpirationdate.json"},
		"name":{
			"type":"string",
			"minLength":2
		},
		"created_at":{"$ref": "../definitions/iso8601.json"},
		"updated_at":{"$ref": "../definitions/iso8601.json"}
	},
	"required":["account", "address", "number", "last_four", "expiration", "ccv", "name", "created_at", "updated_at"]
}
