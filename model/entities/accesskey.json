{
	"$schema": "http://json-schema.org/schema#",
	"id":"/accesskey",
	"title": "Access Key",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		},
		"OptionalUUIDv4":{
			"anyOf":[
				{"type":"string", "enum":[""]},
				{"$ref": "#/definitions/UUIDv4"}
			]
		}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"access_key":{
			"type":"string",
			"minLength":3
		},
		"secret_key":{
			"type":"string",
			"minLength":3
		},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":["access_key", "secret_key", "created_at", "updated_at"]
}
