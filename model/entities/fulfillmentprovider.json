{
	"$schema": "http://json-schema.org/schema#",
	"id":"/fulfillmentprovider",
	"title": "fulfillment provider",
	"type": "object",
	"definitions": {
		"UUIDv4": {"$ref": "../definitions/uuidv4.json"},
		"ISO8601": {"$ref": "../definitions/iso8601.json"}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"name": {
			"type":"string"
		},
		"username": {
			"type":"string"
		},
		"password": {
			"type":"string"
		},
		"endpoint": {
			"type":"string",
			"format":"uri"
		},
		"provider":{
			"type":"string"
		},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":[ "name", "username", "password", "endpoint", "provider", "created_at", "updated_at"]
}
