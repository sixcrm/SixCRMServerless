{
	"$schema": "http://json-schema.org/schema#",
	"id":"/fulfillmentprovider",
	"title": "fulfillment provider",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"name": {
			"type":"string"
		},
		"username": {
			"type":"string"
		},
		"password": {
			"type":"string"
		},
		"endpoint": {
			"type":"string",
			"format":"uri"
		},
		"provider":{
			"type":"string"
		},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":[ "name", "username", "password", "endpoint", "provider", "created_at", "updated_at"]
}
