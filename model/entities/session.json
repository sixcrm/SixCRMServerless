{
  "$schema": "http://json-schema.org/schema#",
	"id":"/Session",
	"title": "Session",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		},
    "SixAccountIdentifier":{
			"anyOf":[
				{"$ref": "#/definitions/UUIDv4"},
				{"type":"string", "enum":["*"]}
			]
		}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"customer":{"$ref": "#/definitions/UUIDv4"},
		"campaign":{"$ref": "#/definitions/UUIDv4"},
		"product_schedules":{
			"type": "array",
			"items":{"$ref": "#/definitions/UUIDv4"}
		},
		"affiliate":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_1":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_2":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_3":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_4":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_5":{"$ref": "#/definitions/UUIDv4"},
		"cid":{"$ref": "#/definitions/UUIDv4"},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"},
		"completed":{
			"type":"string"
		}
	},
	"required":["id", "customer", "campaign", "created_at", "updated_at"]
}
