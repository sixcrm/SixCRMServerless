{
	"$schema": "http://json-schema.org/schema#",
	"id":"/usersigningstring",
	"title": "User Signing String",
	"type": "object",
	"definitions": {
		"UUIDv4": {"$ref": "../definitions/uuidv4.json"},
		"ISO8601": {"$ref": "../definitions/iso8601.json"}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"user":{
			"type":"string",
			"format":"email"
		},
		"name":{
			"type":"string"
		},
		"signing_string":{
			"type":"string"
		},
		"used_at":{
			"anyOf":[
				{"$ref": "#/definitions/ISO8601"},
				{"type":"null"}
			]
		},
		"created_at":{"$ref": "#/definitions/ISO8601"},
		"updated_at":{"$ref": "#/definitions/ISO8601"}
	},
	"required":["user", "name", "signing_string", "created_at", "updated_at"]
}
