{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/entities/accountdetails.json",
	"title": "AccountDetails",
	"type": "object",
	"description": "An account details",
	"properties": {
		"id": {
			"$ref": "../definitions/sixcrmaccountidentifier.json",
			"description": "ID of account"
		},
		"company_logo": {
			"type": "string",
			"minLength": 3
		},
		"support_link": {
		  "type": "string",
		  "minLength": 3
		},
		"emailtemplates": {
			"type": "object",
			"properties": {
				"custom_blocks": {
				  "type": "array"
				},
				"color_primary": {
					"type": "string"
				},
				"color_secondary": {
				  "type": "string"
				}
			}
		},
		"created_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"updated_at": {
			"$ref": "../definitions/iso8601.json"
		}
	},
	"required": [
		"id",
		"created_at",
		"updated_at"
	]
}
