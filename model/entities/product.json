{
	"$schema": "http://json-schema.org/schema#",
	"id":"/entities/product.json",
	"title": "Product",
	"type": "object",
	"description": "A product",
	"properties": {
		"id":{"$ref": "../definitions/uuidv4.json"},
		"name":{
			"type":"string",
			"minLength":3
		},
		"description":{
			"type":"string"
		},
		"sku":{
			"type":"string",
			"minLength":3
		},
		"ship":{
			"type":"boolean"
		},
		"shipping_delay":{"$ref": "../definitions/nonnegativeinteger.json"},
		"merchantprovidergroup":{"$ref": "../definitions/uuidv4.json"},
		"fulfillment_provider":{"$ref": "../definitions/uuidv4.json"},
		"default_price":{"$ref": "../definitions/currency.json"},
		"dynamic_pricing":{"$ref": "./components/productdynamicpricing.json"},
		"attributes":{"$ref": "./components/productattributes.json"},
		"account":{"$ref": "../definitions/sixcrmaccountidentifier.json"},
		"created_at":{"$ref": "../definitions/iso8601.json"},
		"updated_at":{"$ref": "../definitions/iso8601.json"}
	},
	"required":["name", "account", "created_at", "updated_at"]
}
