{
  "$schema": "http://json-schema.org/schema#",
  "id": "/definitions/currency.json",
  "title": "Currency",
  "description": "Amount of money.",
  "anyOf":[
    {
      "type":"string",
      "pattern":"^\\$*[0-9]+\\.[0-9]{2}$"
    },
    {
      "type": "number",
      "minimum":0.00
    }
  ]
}
