{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/definitions/linearmeasurement.json",
	"title": "Linear Measurement",
	"type": "object",
	"properties": {
		"units": {
			"$ref": "./nonnegativefloat.json"
		},
		"unitofmeasurement": {
			"type": "string",
			"enum": [
				"inches",
				"feet",
				"yards",
				"meters",
				"centimeters",
				"millimeters"
			]
		}
	},
	"required": [
		"units",
		"unitofmeasurement"
	]
}