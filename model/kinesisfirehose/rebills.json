{
	"$schema": "http://json-schema.org/schema#",
	"id":"/RedshiftEventsFirehoseObject",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		}
	},
	"properties": {
    "id_rebill":{"$ref": "#/definitions/UUIDv4"},
		"current_queuename":{"type":"string","enum": ["bill", "hold", "pending", "shipped", "delivered", "recover", "failed","fail_hold",
			"pending", "fail_shipped", "fail_delivered", "fail_recover"
		]},
    "previous_queuename":{"type":"string","enum": ["bill", "hold", "pending", "shipped", "delivered", "recover", "failed","fail_hold",
			"pending", "fail_shipped", "fail_delivered", "fail_recover"
		]},
		"account":{"$ref": "#/definitions/UUIDv4"},
		"datetime": {"$ref": "#/definitions/ISO8601"}
	},
	"required":["id_rebill","current_queuename","account","previous_queuename","datetime"]
}
