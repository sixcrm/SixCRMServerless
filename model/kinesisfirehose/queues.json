{
	"$schema": "http://json-schema.org/schema#",
	"id":"/RedshiftEventsFirehoseObject",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		}
	},
	"properties": {
		"queuename":{"type":"string","enum": ["bill", "hold", "pending", "shipped", "delivered", "recover", "failed"]},
		"account":{"$ref": "#/definitions/UUIDv4"},
		"count":{"type":"number"},
		"datetime": {"$ref": "#/definitions/ISO8601"}
	},
	"required":["account","count","account","datetime"]
}
