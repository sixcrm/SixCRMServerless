{
	"$schema": "http://json-schema.org/schema#",
	"id":"/RedshiftTransactionsFirehoseObject",
	"type": "object",
	"definitions": {
		"UUIDv4": {
			"type": "string",
			"pattern":"^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}$"
		},
		"ISO8601": {
			"type": "string",
			"pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]+)?(([+-][0-9]{2}:[0-9]{2})|Z)?$"
		}
	},
	"properties": {
		"id":{"$ref": "#/definitions/UUIDv4"},
		"customer":{"$ref": "#/definitions/UUIDv4"},
		"creditcard":{"$ref": "#/definitions/UUIDv4"},
		"merchant_processor":{"$ref": "#/definitions/UUIDv4"},
		"campaign":{"$ref": "#/definitions/UUIDv4"},
		"affiliate":{"$ref": "#/definitions/UUIDv4"},
		"account":{"$ref": "#/definitions/UUIDv4"},
		"product_schedule":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_1":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_2":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_3":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_4":{"$ref": "#/definitions/UUIDv4"},
		"subaffiliate_5":{"$ref": "#/definitions/UUIDv4"},
		"datetime": {"$ref": "#/definitions/ISO8601"},
		"processor_result":{
			"type":"string",
			"enum": ["success", "decline","error"]
		},
		"transaction_type":{
			"type":"string",
			"enum": ["new", "rebill"]
		},
		"subtype":{
			"type":"string",
			"enum": ["upsell", "main"]
		},
		"amount":{
			"type":"number"
		}
	},
	"required":["id", "customer", "creditcard","merchant_processor","campaign","affiliate","account","product_schedule","subaffiliate_1","subaffiliate_2","subaffiliate_3","subaffiliate_4","subaffiliate_5","datetime","processor_result","transaction_type","subtype","amount"]
}
