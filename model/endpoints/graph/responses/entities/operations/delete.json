{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "/deleteentity",
  "type": "object",

  "allOf": [
    { "$ref": "../../graph" },
    {
      "properties": {

        "response" :{

          "properties": {

            "data": {

              "patternProperties": {

                "delete.+": {

                  "properties": {

                    "id": {

                      "type": "string"

                    }

                  }

                }

              },
              "maxProperties": 1,
              "minProperties": 1

            }

          },
          "required":["data"]

        }

      },
      "required":["response"]
    }
  ]

}
