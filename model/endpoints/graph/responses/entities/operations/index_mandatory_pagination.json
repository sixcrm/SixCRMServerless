{
  "$schema": "http://json-schema.org/schema#",
  "id": "/listentity",
  "title": "List of entities",
  "type": "object",
  "allOf": [
    { "$ref": "../../graph" },
    {
      "properties": {
        "response" :{
          "properties": {
            "data": {
              "patternProperties": {
                ".+list": {
                  "patternProperties": {
                    ".+s": {
                      "anyOf":[
                        {
                          "type": "array",
                          "items": { "$ref": "../entity" }
                        },
                        {
                          "type":"null"
                        }
                      ]
                    }
                  },
                  "properties": {
                    "pagination": { "$ref": "../../dynamo_pagination" }
                  },
                  "required":["pagination"],
                  "maxProperties": 2,
                  "minProperties": 2
                }
              },
              "required":[]
            }
          },
          "required":["data"]
        }
      },
      "required":["response"]
    }
  ]

}
