{
  "$schema": "http://json-schema.org/schema#",
  "id": "/graph",
  "title": "Graph Response",
  "type": "object",

  "properties": {
    "success": {
      "type": "boolean"
    },
    "code": {
      "type": "number",
      "minimum": 200,
      "maximum": 599
    },
    "error_type": {
      "type": "string"
    },
    "response" :{
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "data": {
              "type": "object"
            }
          }
        },
        {
          "type": "null"
        },
        { "$ref": "definitions/jwt" }
      ]

    }
  },
  "required":["success", "code", "response"]
}
