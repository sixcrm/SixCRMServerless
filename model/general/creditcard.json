{
	"title": "CreditCard",
	"type": "object",
	"definitions": {
		"CreditCardNumber": {
			"anyOf": [
		    {
					"type": "string"
				}
		  ]
		},
		"CreditCardExpirationDate": {
			"type": "string",
			"pattern":"^(0[1-9]|1[0-2])\/?([0-9]{4}|[0-9]{2})$"
		},
		"CreditCardCCV":{
			"type":"string",
			"pattern":"^[0-9]{3,4}$"
		}
	},
	"properties": {
		"number":{"$ref": "#/definitions/CreditCardNumber"},
		"expiration":{"$ref": "#/definitions/CreditCardNumber"},
		"ccv":{"$ref": "#/definitions/CreditCardCCV"},
		"name":{
			"type":"string"
		},
		"address":{"$ref": "/Address"}
	},
	"required":["number","expiration","ccv","name","address"]
}
