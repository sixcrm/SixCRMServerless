{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/authorization/decodedauthorizationtoken.json",
	"title": "Decoded Authorization Token",
	"type": "object",
	"properties": {
		"customer":{"$ref": "../definitions/uuidv4.json"},
		"email": {
			"type": "string",
			"format": "email"
		},
		"email_verified": {
			"type": "boolean"
		},
		"picture": {
			"type": "string"
		},
		"iss": {
			"type": "string",
			"format": "uri"
		},
		"sub": {
			"type": "string"
		},
		"aud": {
			"type": "string"
		},
		"exp": {
			"$ref": "../definitions/nonnegativeinteger.json"
		},
		"iat": {
			"$ref": "../definitions/nonnegativeinteger.json"
		}
	}
}
