{
	"$schema": "http://json-schema.org/draft-04/schema",
	"id":"/usersigningstrings",
	"title": "User Signing Strings",
	"type": "array",
	"items":{
		"id":"/usersigningstring",
		"title": "User Signing String",
		"type": "object",
		"properties": {
			"id":{"$ref": "../definitions/uuidv4.json"},
			"user":{"$ref": "../definitions/useridentifier.json"},
			"name":{
				"type":"string"
			},
			"signing_string":{"$ref": "../definitions/signingstring.json"},
			"used_at":{
				"anyOf":[
					{"$ref": "../definitions/iso8601.json"},
					{"type":"null"}
				]
			},
			"created_at":{"$ref": "../definitions/iso8601.json"},
			"updated_at":{"$ref": "../definitions/iso8601.json"}
		},
		"required":["user", "name", "signing_string", "created_at", "updated_at"]
	}
}
