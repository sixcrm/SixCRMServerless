  "scripts": {
    "test-environment": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/environment/*.test.js",
    "test-miscellaneous": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/miscellaneous/*.test.js",
    "test-entities": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/entities/*.test.js",
    "test-transactions": "node ./node_modules/mocha/bin/mocha --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/transaction/*.test.js",
    "test-deleteblocks": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/deleteblocks/*.test.js",
    "test-integration": "node ./node_modules/mocha/bin/mocha --retries 3 -- test/config/global.js test/bootstrap.test.js test/integration/graph/**/*.test.js test/integration/transaction/*.test.js",
    "test-unit": "node_modules/.bin/mocha --bail --require test/unit/dependencies.js --recursive './test/unit/**/*.spec.js'",
    "test-unit-circle": "nyc node_modules/.bin/mocha --bail --require test/unit/dependencies.js --recursive './test/unit/**/*.spec.js'",
    "test-local-integration": "mocha ./test/integration/local/createOrder/createOrder.test.js",
    "test-functional": "AWS_PROFILE=six nyc node_modules/.bin/_mocha --recursive test/config/global.js './test/functional/*.test.js'",
    "test-functional-state-machine": "AWS_PROFILE=six node ./node_modules/mocha/bin/mocha --retries 3 --require test/config/global.js ./test/functional/stateMachine.test.js",
    "test-functional-state-machine-docker": "AWS_PROFILE=six node --max_old_space_size=4096 ./node_modules/mocha/bin/mocha --retries 0 --require test/config/global.js ./test/functional/state_machine/**/*.test.js",
    "test-functional-connectivity": "AWS_PROFILE=six node ./node_modules/mocha/bin/mocha --retries 3 ./test/functional/functional-connectivity.spec.js",
    "test-functional-redis-provider": "node ./node_modules/mocha/bin/mocha --require test/unit/dependencies.js ./test/functional/redis-provider.spec.js",
    "test-functional-analytics": "npm run test-functional-analytics-queries && npm run test-functional-analytics-events",
    "test-functional-analytics-events": "AWS_PROFILE=six nyc node_modules/.bin/_mocha --recursive test/config/global.js './test/functional/analytics/events/**/*.test.js'",
    "test-functional-analytics-queries": "AWS_PROFILE=six node test/functional/analytics/queries/aurora-queries.js",
    "test-integration-analytics": "node ./node_modules/mocha/bin/mocha --recursive --bail--retries 3 --require test/config/global.js test/bootstrap.test.js 'test/integration/graph/analytics/**/*.test.js'",
    "test-analytics-seed": "node ./test/functional/analytics/seeds/seed.js ",
    "test-workers": "nyc node_modules/.bin/_mocha  -- --require test/config/global.js test/bootstrap.test.js --recursive './test/unit/workers/**/*.spec.js'",
    "test-endpoints": "nyc node_modules/.bin/_mocha  -- --require test/config/global.js test/bootstrap.test.js './test/unit/endpoints/**/*.spec.js'",
    "lint-eslint": "eslint --ignore-pattern 'coverage/**' --ignore-pattern '**/*test.js' --ignore-pattern '**/*spec.js' '{**/*.js,**/*.json}'",
    "lint-yml": "node_modules/yaml-lint/cli.js **/*.yml",
    "lint": "npm run lint-eslint && npm run lint-yml",
    "lint-commit": "./tools/lintCommitEslint.sh && ./tools/lintCommitYml.sh"
  },
  "pre-commit": [
    "lint",
    "test-unit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimothyDalbey/sixcrmtransactionapi.git"
  },
  "author": "Timothy Dalbey",
