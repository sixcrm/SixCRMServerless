    "serverless-dynamodb-local": "0.2.18",
    "serverless-mocha-plugin": "^1.3.2",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-prune-plugin": "^1.1.1",
    "supertest": "^2.0.1",
    "yaml-lint": "0.0.4"
  },
  "scripts": {
    "test-miscellaneous": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/miscellaneous/*.test.js",
    "test-entities": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/entities/*.test.js",
    "test-analytics": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/analytics/*.test.js",
    "test-transactions": "node ./node_modules/mocha/bin/mocha --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/transaction/*.test.js",
    "test-deleteblocks": "node ./node_modules/mocha/bin/mocha --bail --retries 3 --require test/config/global.js test/bootstrap.test.js test/integration/graph/deleteblocks/*.test.js",
    "test-integration": "node ./node_modules/mocha/bin/mocha --bail --retries 3 -- test/config/global.js test/bootstrap.test.js test/integration/graph/**/*.test.js test/integration/transaction/*.test.js",
    "test-unit": "istanbul cover node_modules/.bin/_mocha --bail --require test/unit/dependencies.js --recursive './test/unit/**/*.spec.js'",
    "test-local-integration": "mocha ./test/integration/local/createOrder/createOrder.test.js",
    "test-functional": "AWS_PROFILE=six istanbul cover node_modules/.bin/_mocha --recursive test/config/global.js './test/functional/*.test.js'",
    "test-functional-state-machine": "AWS_PROFILE=six node ./node_modules/mocha/bin/mocha --retries 3 --require test/config/global.js ./test/functional/stateMachine.test.js",
    "test-functional-state-machine-docker": "AWS_PROFILE=six node --max_old_space_size=4096 ./node_modules/mocha/bin/mocha --retries 0 --require test/config/global.js ./test/functional/state_machine/**/*.test.js",
    "test-functional-connectivity": "AWS_PROFILE=six node ./node_modules/mocha/bin/mocha --retries 3 ./test/functional/functional-connectivity.spec.js",
    "test-functional-redis-utilities": "node ./node_modules/mocha/bin/mocha --require test/unit/dependencies.js ./test/functional/redis-utilities.spec.js",
    "test-queries": "AWS_PROFILE=six istanbul cover node_modules/.bin/_mocha --recursive test/config/global.js './test/functional/queries/**/*.test.js'",
    "test-workers": "istanbul cover node_modules/.bin/_mocha  -- --require test/config/global.js test/bootstrap.test.js --recursive './test/unit/workers/**/*.spec.js'",
    "test-endpoints": "istanbul cover _mocha  -- --require test/config/global.js test/bootstrap.test.js './test/unit/endpoints/**/*.spec.js'",
    "lint-eslint": "eslint --ignore-pattern 'coverage/**' --ignore-pattern '**/*test.js' --ignore-pattern '**/*spec.js' '{**/*.js,**/*.json}'",
    "lint-yml": "node_modules/yaml-lint/cli.js **/*.yml",
    "lint": "npm run lint-eslint && npm run lint-yml",
    "lint-commit": "./tools/lintCommitEslint.sh && ./tools/lintCommitYml.sh"
  },
  "pre-commit": [
    "lint-commit",
    "test-unit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimothyDalbey/sixcrmtransactionapi.git"
  },
  "author": "Timothy Dalbey",
  "license": "ISC",
  "homepage": "https://github.com/TimothyDalbey/sixcrmtransactionapi#readme"
}
