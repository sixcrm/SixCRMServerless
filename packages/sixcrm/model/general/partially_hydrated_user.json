{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/general/partially_hydrated_user.json",
	"title": "Partially Hydrated User",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"active": {
			"type": "string"
		},
		"termsandconditions": {
			"type": [
				"string",
				"null"
			]
		},
		"acl": {
			"type": "array",
			"minItems": 1,
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"account": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"name": {
								"type": "string"
							},
							"active": {
								"type": "string"
							}
						},
						"required": [
							"id",
							"name",
							"active"
						]
					},
					"role": {
						"type": "object",
						"properties": {
							"id": {
								"type": "string"
							},
							"name": {
								"type": "string"
							}
						},
						"required": [
							"id",
							"name",
							"active"
						]
					}
				},
				"required": [
					"id",
					"account",
					"role"
				]
			}
		}
	},
	"required": [
		"id",
		"name",
		"termsandconditions",
		"acl",
		"active"
	]
}
