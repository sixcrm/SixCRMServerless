{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/definitions/zipcode.json",
	"title": "ZIP Code",
	"description": "A ZIP code",
	"anyOf": [
		{
			"type": "string",
			"pattern": "^[0-9]{5}$"
		},
		{
			"type": "string",
			"pattern": "^[0-9]{5}-[0-9]{4}$"
		},
		{
			"type": "number",
			"minimum": 10000,
			"maximum": 99999
		}
	]
}