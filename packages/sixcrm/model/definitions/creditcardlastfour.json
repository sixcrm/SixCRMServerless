{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/definitions/creditcardlastfour.json",
	"title": "Credit Card Last Four",
	"description": "The last four digits of a credit card number",
	"anyOf": [
		{
			"type": "string",
			"pattern": "^[0-9]{4}$"
		},
		{
			"type": "integer",
			"minimum": 100000,
			"maximum": 999999
		}
	]
}