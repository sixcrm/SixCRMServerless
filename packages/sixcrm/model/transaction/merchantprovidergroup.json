{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "model/transaction/merchantprovidergroup.json",
	"title": "Merchant Provider Group",
	"type": "object",
	"properties": {
		"id": {
			"$ref": "../definitions/uuidv4.json"
		},
		"account": {
			"$ref": "../definitions/sixcrmidentifier.json"
		},
		"name": {
			"type": "string"
		},
		"merchantproviders": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"$ref": "../definitions/uuidv4.json"
					},
					"distribution": {
						"type": "number"
					}
				},
				"required": [
					"id",
					"distribution"
				]
			}
		},
		"created_at": {
			"$ref": "../definitions/iso8601.json"
		},
		"updated_at": {
			"$ref": "../definitions/iso8601.json"
		}
	},
	"required": [
		"account",
		"created_at",
		"updated_at"
	]
}