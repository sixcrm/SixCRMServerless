  "scripts": {
    "test-environment": "mocha --retries 3 --colors --require test/config/global.js test/bootstrap.test.js test/integration/environment/*.test.js",
    "test-miscellaneous": "mocha --retries 3 --colors --require test/config/global.js test/bootstrap.test.js test/integration/graph/miscellaneous/*.test.js",
    "test-entities": "mocha --retries 3 --colors --require test/config/global.js test/bootstrap.test.js test/integration/graph/entities/*.test.js",
    "test-live-transactions": "mocha --colors --require test/config/global.js test/bootstrap.test.js test/live-transactions/*.test.js",
    "test-transactions": "mocha --retries 3 --colors --require test/config/global.js test/bootstrap.test.js test/integration/transaction/*.test.js",
    "test-deleteblocks": "mocha --retries 3 --colors --require test/config/global.js test/bootstrap.test.js test/integration/graph/deleteblocks/*.test.js",
    "test-integration": "mocha --retries 3 -- test/config/global.js test/bootstrap.test.js test/integration/graph/**/*.test.js test/integration/transaction/*.test.js",
    "test-unit": "mocha --colors --require test/unit/dependencies.js --recursive './test/unit/**/*.spec.js'",
    "test-unit-bail": "mocha --bail --colors --require test/unit/dependencies.js --recursive './test/unit/**/*.spec.js'",
    "test-unit-circle": "nyc mocha --colors --require test/unit/dependencies.js --recursive --max-old-space-size=4096 './test/unit/**/*.spec.js'",
    "test-local-integration": "mocha --colors ./test/integration/local/createOrder/createOrder.test.js",
    "test-integration-analytics": "mocha --recursive --retries 3 --colors --require test/config/global.js test/bootstrap.test.js 'test/integration/graph/analytics/**/*.test.js'",
    "lint-eslint": "eslint --color '{**/*.js,**/*.json}'",
    "lint-yml": "yamllint **/*.yml",
    "lint": "npm run lint-eslint && npm run lint-yml",
    "lint-commit": "./tools/lintCommitEslint.sh && ./tools/lintCommitYml.sh",
    "test-cli-extract": "NODE_CONFIG_DIR=config/local NODE_CONFIG_ENV=site SIX_DEBUG_LOCAL=true SIX_VERBOSE=3 node cli/index.js extract --crm limelight --client elyfe.limelightcrm.com",
    "test-cli-ingest": "stage=local-docker NODE_CONFIG_DIR=config/local-docker NODE_CONFIG_ENV=site SIX_DEBUG_LOCAL=true SIX_VERBOSE=2 node cli/index.js ingest --account bb399f83-a050-46dd-a14d-b8b1b444c8b8",
    "ts-watch": "tsc -w",
    "ts-build": "tsc --pretty",
    "compile": "yarn ts-build",
    "ts-test": "mocha --colors -r ts-node/register **/*.spec.ts"
